{
    "singleQuote": true,
    "trailingComma": "none",
    "endOfLine": "auto",
    "tabWidth": 4,
    "semi": true,
    "bracketSpacing": true,
    "bracketSameLine": true,
    "overrides": [
        {
            "files": ["*.ts"],
            "options": {
                "printWidth": 280
            }
        },
        {
            "files": ["*.html"],
            "options": {
                "printWidth": 280
            }
        }
    ]
}
